{
  "blocks": false,
  "brackets": "never",
  "colons": "always",
  "colors": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "depthLimit": false,
  "duplicates": {
    "expect": true,
    "error": true
  },
  "efficient": "always",
  "extendPref": false,
  "globalDupe": false,
  "indentPref": false,
  "leadingZero": {
    "expect": "never",
    "error": true
  },
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": false,
  "namingConvention": {
    "expect": "lowercase-dash",
    "error": true
  },
  "namingConventionStrict": {
    "expect": true,
    "error": true
  },
  "none": {
    "expect": "never",
    "error": true
  },
  "noImportant": {
    "expect": true,
    "error": true
  },
  "parenSpace": false,
  "placeholders": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": false,
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "sortOrder": {
    "expect": "grouped",
    "error": true
  },
  "stackedProperties": {
    "expect": "never",
    "error": true
  },
  "trailingWhitespace": "never",
  "universal": false,
  "valid": true,
  "zeroUnits": "never",
  "zIndexNormalize": false,
  "reporter": "stylint-stylish"
}
